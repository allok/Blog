@{
    Layout = null;
}
@model blogik.Models.TagModel
<li class="block">
    <h4 class="heading">Категории</h4>
    <div class="small-posts-list ">
        <div class="widget-block">
            <ul>
                @foreach (var item in Model.tag_mas)
                {
                    <li class="cat-item"><a href="@Url.RouteUrl("Tags", new { Controller = "Tags", action = "NTags", id = @item } )">@item</a></li>
                }
            </ul>
        </div>
    </div>
</li>